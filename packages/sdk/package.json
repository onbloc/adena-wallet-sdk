{
  "name": "@adena-wallet/sdk",
  "version": "0.0.12",
  "description": "Adena Wallet SDK",
  "license": "MIT",
  "author": "Onbloc Co., Ltd. <info@adena.app>",
  "homepage": "https://www.adena.app",
  "main": "./bin/index.js",
  "module": "./bin/index.mjs",
  "types": "./bin/index.d.ts",
  "exports": {
    ".": {
      "types": "./bin/index.d.ts",
      "import": "./bin/index.mjs",
      "require": "./bin/index.js"
    }
  },
  "files": [
    "bin/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onbloc/adena-wallet-sdk",
    "directory": "packages/sdk"
  },
  "keywords": [
    "adena",
    "wallet",
    "gno",
    "sdk",
    "js"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "prettier src/ --check && eslint",
    "lint:fix": "prettier src/ --write && eslint --fix",
    "test": "jest",
    "test:ci": "jest --coverage --passWithNoTests "
  },
  "dependencies": {
    "@gnolang/gno-js-client": "1.4.5",
    "@gnolang/tm2-js-client": "1.3.3",
    "@noble/curves": "1.9.4",
    "@noble/hashes": "1.8.0",
    "@scure/bip32": "1.7.0",
    "@scure/bip39": "1.6.0",
    "@web3auth/auth-adapter": "9.5.3",
    "@web3auth/base": "9.5.3",
    "@web3auth/base-provider": "9.5.3",
    "@web3auth/no-modal": "9.5.3",
    "@web3auth/openlogin-adapter": "8.12.4",
    "ethereum-cryptography": "3.2.0"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "2.2.3",
    "@types/eslint": "9",
    "@types/jest": "29.5.12",
    "@types/node": "22.0.0",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "eslint": "9.8.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.3",
    "ts-node": "10.9.2",
    "tsup": "8.2.3",
    "typescript": "5.5.4"
  }
}
